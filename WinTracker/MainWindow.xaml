<ui:FluentWindow x:Class="WinTracker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:Controls="clr-namespace:WinTracker.Views"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Title="Application Usage Tracker" Height="450" Width="800"
        xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
        d:DataContext="{d:DesignInstance local:MainWindow, IsDesignTimeCreatable=True}">
    <Window.Resources>
        <ResourceDictionary Source="Dictionary.xaml">
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <ui:SymbolIcon Symbol="Fluent24"/>
        <Frame Content="{Binding CurrentView}"></Frame>
    </Grid>
</ui:FluentWindow>

